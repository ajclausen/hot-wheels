{
  "name": "hotwheels-collection-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"wrangler pages dev --proxy=5173\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pages:deploy": "npm run build && wrangler pages deploy ./dist",
    "db:init": "wrangler d1 execute hotwheels-collection --local --file=./schema.sql",
    "db:create": "wrangler d1 create hotwheels-collection"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "hono": "^4.6.9",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "wrangler": "^3.86.1"
  }
}